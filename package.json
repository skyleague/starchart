{
  "name": "@skyleague/starchart",
  "version": "1.2.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/skyleague/starchart.git"
  },
  "exports": {
    ".": "./.dist/index.js",
    "./package.json": "./package.json",
    "./*.js": "./.dist/*.js"
  },
  "types": "./.dist/index.d.ts",
  "files": ["bin", ".dist", "package.json"],
  "bin": {
    "starchart": "bin/run.js"
  },
  "scripts": {
    "check:project": "node-standards lint",
    "lint": "biome check",
    "lint:fix": "npm run lint -- --write --unsafe",
    "prepare": "husky || true",
    "build": "tsc -p tsconfig.dist.json",
    "build:docs": "npx typedoc",
    "check:coverage": "vitest run --coverage=true",
    "check:types": "tsc -p tsconfig.json",
    "package": "rm -rf .dist && npm run build",
    "prepack": "cp package.json .dist/package.json",
    "test": "vitest run"
  },
  "dependencies": {
    "@aws-sdk/client-iam": "^3.637.0",
    "@aws-sdk/client-lambda": "^3.637.0",
    "@skyleague/esbuild-lambda": "^5.3.2",
    "@skyleague/event-horizon": "^9.0.0",
    "aws-iot-device-sdk-v2": "^1.20.0",
    "camelcase": "^8.0.0",
    "find-root": "^1.1.0",
    "globby": "^14.0.2",
    "js-yaml": "^4.1.0",
    "pino-pretty": "^11.2.1",
    "tsx": "^4.16.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@aws-sdk/client-iot": "^3.637.0",
    "@aws-sdk/credential-providers": "^3.637.0",
    "@skyleague/node-standards": "^7.3.1",
    "@skyleague/therefore": "^5.9.2",
    "@types/aws-lambda": "^8.10.141",
    "@types/find-root": "^1.1.4",
    "@types/js-yaml": "^4.0.9",
    "@types/yargs": "^17.0.32",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "node-standards": {
    "extends": "yargs-cli"
  }
}
